  <exercise label="posets_PI_antichain-type">
    <statement>
      <p>The set <m>\{12,13,30,16\}</m> is
      </p>
      <image source="big-poset" width="65%">
        <description>A large poset</description>
      </image>
    </statement>
    <choices>
      <choice>
        <statement>
          <p>a maximal antichain (not maximum).</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>a maximum antichain.</p>
        </statement>
      </choice>
      <choice>
        <statement correct="yes">
          <p>an antichain, but neither maximal nor maximum.</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>not an antichain.</p>
        </statement>
      </choice>
    </choices>
  </exercise>  
