<exercises xml:id="s_inclusion-exclusion_PI" component="runestone-PI">
  <title>Peer Instruction Questions</title>
  <exercise label="inclusion-exclusion_PI_IE-sum-terms">
    <statement>
      <p>Suppose that <m>m=4</m>. How many terms are there in the sum <me>\displaystyle
        \sum_{S\subseteq [m]} (-1)^{|S|}N(S)</me>?</p>
    </statement>
    <choices>
      <choice>
        <statement>
          <p>
            <m>2^{4-1} = 8 </m>
          </p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>
            <m>2^{4}-1 = 15 </m>
          </p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p>
            <m>2^{4} = 16 </m>
          </p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>
            <m>(4-1)! = 6 </m>
          </p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>
            <m>4! = 24 </m>
          </p>
        </statement>
      </choice>
    </choices>
  </exercise>
  <exercise label="inclusion-exclusion_PI_surjection">
