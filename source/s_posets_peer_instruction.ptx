<exercises xml:id="s_posets_PI" component="runestone-PI">
  <title>Peer Instruction Questions</title>
  <exercise label="posets_PI_movie-poset">
    <statement correct="yes">
      <p>I understood how Bob constructed his picture in the class preparation assignment.
      </p>
      <image width="65%" source="movies">
        <description>A poset ranking seven movies</description>
      </image>
    </statement>
  </exercise>
  <exercise label="posets_PI_antichains">
    <statement>
      <p>Which of the following sets are antichains?
      </p>
      <image source="big-poset" width="65%">
        <description>A large poset</description>
      </image>
    </statement>
    <choices multiple-mark="yes">
      <choice>
        <statement correct="yes">
          <p><m>\{20,7,32,15\}</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>\{9,7,32,6\}</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>\{19,27,6,35\}</m></p>
        </statement>
      </choice>
      <choice>
        <statement correct="yes">
          <p><m>\{9,25,26,7,12\}</m></p>
        </statement>
      </choice>
    </choices>
  </exercise>  
