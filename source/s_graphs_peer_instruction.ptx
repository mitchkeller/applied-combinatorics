  <exercise label="graphs_PI_isomorphic1">
    <statement>
      <p>
        Graphs <m>\bfG</m> and <m>\bfH</m> are isomorphic.
      </p>
      <image source="isomorphic_graphs" width="85%">
        <description>Two graphs</description>
      </image>
    </statement>
    <choices>
      <choice correct="yes">
        <statement>
          <p>True, high confidence</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>True, low confidence</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>False, low confidence</p>
        </statement>
      </choice>
      <choice >
        <statement>
          <p>False, high confidence</p>
        </statement>
      </choice>
    </choices>
  </exercise>
