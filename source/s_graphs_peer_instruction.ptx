    </statement>
    <choices>
      <choice>
        <statement>
          <p><m>\bfH</m> is a subgraph of <m>\bfG</m></p>
        </statement>
      </choice>
      <choice >
        <statement>
          <p><m>\bfH</m> is an induced subgraph of <m>\bfG</m></p>
        </statement>
      </choice>
      <choice correct="yes">
