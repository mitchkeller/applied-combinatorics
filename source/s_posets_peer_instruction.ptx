  <exercise label="posets_PI_17-pt-chain">
    <statement>
      <p>If we find a chain <m>C</m> with 17 points in a poset <m>\bfP</m>, then we know
      </p>
    </statement>
    <choices>
      <choice>
        <statement>
          <p><m>\height(\bfP) = 17</m>.</p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>\height(\bfP) \leq 17</m>.</p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p><m>\height(\bfP) \geq 17</m>.</p>
        </statement>
      </choice>
      <choice >
        <statement>
          <p>Absolutely nothing about <m>\height(\bfP)</m></p>
        </statement>
      </choice>
    </choices>
  </exercise>    
  <exercise label="posets_PI_points-subset-lattice">
    <statement>
      <p>How many points are there in <m>\mathbf{2}^n</m>?</p>
    </statement>
    <choices>
      <choice>
        <statement>
          <p><m>n</m></p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p><m>2^n</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>n!</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>None of the above</p>
        </statement>
      </choice>
    </choices>
  </exercise>
  <exercise label="posets_PI_subset-lattice-minimal">
