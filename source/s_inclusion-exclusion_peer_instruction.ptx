  <exercise label="inclusion-exclusion_PI_functions-count">
    <statement>
      <p>Let <m>X</m> be the set of all functions from <m>[m]</m> to <m>[n]</m>. What is the size of <m>X</m>?</p>
    </statement>
    <choices>
      <choice>
        <statement>
          <p><m>n^m</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>n\cdot m</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>P(n,m)</m></p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p><m>m^n</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p><m>P(m,n)</m></p>
        </statement>
      </choice>
    </choices>
  </exercise>
