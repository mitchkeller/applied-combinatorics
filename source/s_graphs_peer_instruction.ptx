  <title>Peer Instruction Questions</title>
  <exercise label="graphs_PI_subgraph1">
    <statement>
      <p>Choose the best response.
      </p>
      <image source="spanning_subgraph" width="85%">
        <description>Two graphs</description>
      </image>
    </statement>
    <choices>
      <choice>
        <statement>
          <p><m>\bfH</m> is a subgraph of <m>\bfG</m></p>
        </statement>
      </choice>
      <choice >
        <statement>
          <p><m>\bfH</m> is an induced subgraph of <m>\bfG</m></p>
        </statement>
      </choice>
      <choice correct="yes">
        <statement>
          <p><m>\bfH</m> is a spanning subgraph of <m>\bfG</m></p>
        </statement>
      </choice>
      <choice>
        <statement>
          <p>None of the above</p>
        </statement>
      </choice>
    </choices>
  </exercise>
